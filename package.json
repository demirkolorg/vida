{
  "name": "vida",
  "version": "2.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon server/server.js",
    "devall": "nodemon server/server.js && cd client && npm run dev",
    "dev:server": "nodemon server/server.js",
    "dev:client": "cd client && npm run dev",
    "build": "cd server && npm install  && cd ../client && npm install && npm run build",
    "start": "node server/server.js",
    "prisma:format": "prisma format --schema=./server/prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema=./server/prisma/schema.prisma",
    "prisma:generate": "prisma generate --schema=./server/prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema=./server/prisma/schema.prisma",
    "db:push": "prisma db push --schema=./server/prisma/schema.prisma",
    "db:seed": "prisma db seed",
    "prisma": "npm run prisma:format && npm run prisma:migrate && npm run prisma:generate && npm run dev"
  },
  "prisma": {
    "schema": "server/prisma/schema.prisma",
    "seed": "node server/prisma/seed.js"
  },
